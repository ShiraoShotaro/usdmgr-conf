{
    "terminal.integrated.env.windows": {
        "PATH": "${env:PATH}"
    },
    "glsllint.glslangValidatorArgs": "-V --target-env vulkan1.3",
    "files.associations": {
        "vector": "cpp",
        "algorithm": "cpp",
        "array": "cpp",
        "atomic": "cpp",
        "bit": "cpp",
        "bitset": "cpp",
        "cctype": "cpp",
        "charconv": "cpp",
        "chrono": "cpp",
        "clocale": "cpp",
        "cmath": "cpp",
        "compare": "cpp",
        "concepts": "cpp",
        "cstddef": "cpp",
        "cstdint": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "cstring": "cpp",
        "ctime": "cpp",
        "cwchar": "cpp",
        "exception": "cpp",
        "filesystem": "cpp",
        "format": "cpp",
        "forward_list": "cpp",
        "fstream": "cpp",
        "initializer_list": "cpp",
        "iomanip": "cpp",
        "ios": "cpp",
        "iosfwd": "cpp",
        "iostream": "cpp",
        "istream": "cpp",
        "iterator": "cpp",
        "limits": "cpp",
        "list": "cpp",
        "locale": "cpp",
        "map": "cpp",
        "memory": "cpp",
        "mutex": "cpp",
        "new": "cpp",
        "numbers": "cpp",
        "optional": "cpp",
        "ostream": "cpp",
        "random": "cpp",
        "ratio": "cpp",
        "set": "cpp",
        "sstream": "cpp",
        "stdexcept": "cpp",
        "stop_token": "cpp",
        "streambuf": "cpp",
        "string": "cpp",
        "system_error": "cpp",
        "thread": "cpp",
        "tuple": "cpp",
        "type_traits": "cpp",
        "typeinfo": "cpp",
        "unordered_map": "cpp",
        "unordered_set": "cpp",
        "utility": "cpp",
        "variant": "cpp",
        "xfacet": "cpp",
        "xhash": "cpp",
        "xiosbase": "cpp",
        "xlocale": "cpp",
        "xlocbuf": "cpp",
        "xlocinfo": "cpp",
        "xlocmes": "cpp",
        "xlocmon": "cpp",
        "xlocnum": "cpp",
        "xloctime": "cpp",
        "xmemory": "cpp",
        "xstddef": "cpp",
        "xstring": "cpp",
        "xtr1common": "cpp",
        "xtree": "cpp",
        "xutility": "cpp"
    },
    "files.exclude": {
        "**/__pycache__": true,
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        ".env": true,
        ".mypy_cache": true
    },
    "flake8.cwd": "${workspaceFolder}",
    "flake8.interpreter": [
        "${workspaceFolder}\\.env\\Scripts\\python.exe"
    ],
    "flake8.args": [
        "--max-line-length",
        "120",
        "--ignore",
        "F631,W503"
    ],
    "flake8.importStrategy": "fromEnvironment",
    "autopep8.args": [
        "--max-line-length",
        "120",
        "--aggressive",
        "--aggressive",
        "--ignore",
        "E721",
    ],
    "autopep8.interpreter": [
        "${workspaceFolder}\\.env\\Scripts\\python.exe"
    ],
    "autopep8.importStrategy": "fromEnvironment",
    "isort.check": true,
    "isort.interpreter": [
        "${workspaceFolder}\\.env\\Scripts\\python.exe"
    ],
    "isort.importStrategy": "fromEnvironment",
    "[python]": {
        "editor.defaultFormatter": "ms-python.autopep8",
        "editor.tabSize": 4,
        "editor.formatOnSave": true,
        "editor.formatOnPaste": true,
    },
    "python.terminal.activateEnvironment": true,
    "python.autoComplete.extraPaths": [
        "${workspaceFolder}\\.env\\Lib\\site-packages",
    ],
    "python.languageServer": "Pylance",
    "python.analysis.extraPaths": [
        "${workspaceFolder}\\.env\\Lib\\site-packages"
    ],
    "python.analysis.autoFormatStrings": true,
    "python.analysis.autoImportCompletions": false,
    "python.analysis.diagnosticMode": "workspace",
    "python.analysis.enablePytestSupport": false,
    "editor.formatOnSave": true,
}